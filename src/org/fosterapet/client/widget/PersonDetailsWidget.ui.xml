<!DOCTYPE ui:UiBinder SYSTEM 'http://dl.google.com/gwt/DTD/xhtml.ent'>
<ui:UiBinder
  xmlns:gwt='urn:import:com.google.gwt.user.client.ui'
  xmlns:gxtbutton='urn:import:com.sencha.gxt.widget.core.client.button'
  xmlns:gxtcontainer='urn:import:com.sencha.gxt.widget.core.client.container'
  xmlns:gxtform='urn:import:com.sencha.gxt.widget.core.client.form'
  xmlns:gxt='urn:import:com.sencha.gxt.widget.core.client'
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
>
  <!-- Margin declarations ===================================================================== -->
  <ui:with
    type='com.sencha.gxt.core.client.util.Margins'
    field='margins_0_0_5_0'
  >
    <ui:attributes
      top='0'
      right='0'
      bottom='5'
      left='0'/>
  </ui:with>
  <ui:with
    type='com.sencha.gxt.core.client.util.Margins'
    field='margins_0_5_0_0'
  >
    <ui:attributes
      top='0'
      right='5'
      bottom='0'
      left='0'/>
  </ui:with>
  <!-- Padding declarations ==================================================================== -->
  <ui:with
    type='com.sencha.gxt.core.client.util.Padding'
    field='padding_5_5_5_5'
  >
    <ui:attributes
      top='5'
      right='5'
      bottom='5'
      left='5'/>
  </ui:with>
  <!-- Box layout declarations ================================================================= -->
  <ui:with
    type='com.sencha.gxt.widget.core.client.container.BoxLayoutContainer.BoxLayoutData'
    field='boxLayoutDataMargins_0_0_5_0'
  >
    <ui:attributes margins='{margins_0_0_5_0}'/>
  </ui:with>
  <ui:with
    type='com.sencha.gxt.widget.core.client.container.BoxLayoutContainer.BoxLayoutData'
    field='boxLayoutDataMargins_0_5_0_0'
  >
    <ui:attributes margins='{margins_0_5_0_0}'/>
  </ui:with>
  <!-- ========================================================================================= -->
  <gxtcontainer:FlowLayoutContainer ui:field='flowLayoutContainer'>
    <gxtcontainer:VerticalLayoutContainer ui:field='mainContainer'>
      <gxtcontainer:HBoxLayoutContainer>
        <gxtcontainer:VerticalLayoutContainer>
          <gxtform:FieldLabel itemId='Person.FirstName'/>
          <gxtform:FieldLabel itemId='Person.LastName'/>
          <gxtform:FieldLabel itemId='Person.DisplayName'/>
          <gxtform:FieldLabel itemId='Person.LoginName'/>
          <gxtform:FieldLabel itemId='Person.DateOfBirth'/>
        </gxtcontainer:VerticalLayoutContainer>
        <gxtcontainer:VerticalLayoutContainer>
          <gxtform:FieldLabel itemId='Person.EmailAddress'/>
          <gxtform:FieldLabel itemId='Person.PhoneNumberHome'/>
          <gxtform:FieldLabel itemId='Person.PhoneNumberMobile'/>
          <gxtform:FieldLabel itemId='Person.PhoneNumberOffice'/>
        </gxtcontainer:VerticalLayoutContainer>
      </gxtcontainer:HBoxLayoutContainer>
      <gxtcontainer:HBoxLayoutContainer
        padding="{padding_5_5_5_5}"
        hBoxLayoutAlign="STRETCHMAX"
      >
        <gxtcontainer:child layoutData="{boxLayoutDataMargins_0_5_0_0}">
          <gxtbutton:TextButton
            ui:field='saveButton'
            text='Save Changes'/>
        </gxtcontainer:child>
        <gxtcontainer:child layoutData="{boxLayoutDataMargins_0_5_0_0}">
          <gxtbutton:TextButton
            ui:field='undoButton'
            text='Undo Changes'/>
        </gxtcontainer:child>
      </gxtcontainer:HBoxLayoutContainer>
      <gxtcontainer:HBoxLayoutContainer>
        <gxtcontainer:VBoxLayoutContainer
          padding='{padding_5_5_5_5}'
          vBoxLayoutAlign='STRETCHMAX'
        >
          <gxtcontainer:child layoutData='{boxLayoutDataMargins_0_0_5_0}'>
            <gxtbutton:TextButton
              ui:field='orgButton'
              text='Organizations'/>
          </gxtcontainer:child>
          <gxtcontainer:child layoutData='{boxLayoutDataMargins_0_0_5_0}'>
            <gxtbutton:TextButton
              ui:field='personRelationshipButton'
              text='Relationships'/>
          </gxtcontainer:child>
          <gxtcontainer:child layoutData='{boxLayoutDataMargins_0_0_5_0}'>
            <gxtbutton:TextButton
              ui:field='fosterHistoryButton'
              text='Foster History'/>
          </gxtcontainer:child>
        </gxtcontainer:VBoxLayoutContainer>
        <gxtcontainer:CardLayoutContainer ui:field='cardLayoutContainer'>
          <gxt:ContentPanel ui:field='orgPanel' headerVisible='false'/>
          <gxt:ContentPanel ui:field='personRelationshipPanel' headerVisible='false'/>
          <gxt:ContentPanel ui:field='fosterHistoryPanel' headerVisible='false'/>
        </gxtcontainer:CardLayoutContainer>
      </gxtcontainer:HBoxLayoutContainer>
    </gxtcontainer:VerticalLayoutContainer>
  </gxtcontainer:FlowLayoutContainer>
</ui:UiBinder>