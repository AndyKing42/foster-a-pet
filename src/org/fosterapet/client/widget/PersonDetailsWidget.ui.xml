<!DOCTYPE ui:UiBinder SYSTEM 'http://dl.google.com/gwt/DTD/xhtml.ent'>
<ui:UiBinder
  xmlns:gwt='urn:import:com.google.gwt.user.client.ui'
  xmlns:gxtbutton='urn:import:com.sencha.gxt.widget.core.client.button'
  xmlns:gxtcontainer='urn:import:com.sencha.gxt.widget.core.client.container'
  xmlns:gxtform='urn:import:com.sencha.gxt.widget.core.client.form'
  xmlns:gxt='urn:import:com.sencha.gxt.widget.core.client'
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
>
  <!-- Margin declarations ===================================================================== -->
  <ui:with
    type='com.sencha.gxt.core.client.util.Margins'
    field='margins_0_0_5_0'
  >
    <ui:attributes
      top='0'
      right='0'
      bottom='5'
      left='0'/>
  </ui:with>
  <ui:with
    type='com.sencha.gxt.core.client.util.Margins'
    field='margins_0_5_0_0'
  >
    <ui:attributes
      top='0'
      right='5'
      bottom='0'
      left='0'/>
  </ui:with>
  <!-- Padding declarations ==================================================================== -->
  <ui:with
    type='com.sencha.gxt.core.client.util.Padding'
    field='padding_5_5_5_5'
  >
    <ui:attributes
      top='5'
      right='5'
      bottom='5'
      left='5'/>
  </ui:with>
  <!-- HorizontalLayoutData declarations ======================================================= -->
  <ui:with
    type='com.sencha.gxt.widget.core.client.container.HorizontalLayoutContainer.HorizontalLayoutData'
    field='buttonHLD'
  >
  </ui:with>
  <ui:with
    type='com.sencha.gxt.widget.core.client.container.HorizontalLayoutContainer.HorizontalLayoutData'
    field='fillRemainingHLD'
  >
    <ui:attributes
      height='1'
      width='1'/>
  </ui:with>
  <ui:with
    type='com.sencha.gxt.widget.core.client.container.HorizontalLayoutContainer.HorizontalLayoutData'
    field='sizeToContentsHLD'
  >
    <ui:attributes
      height='-1'
      width='1'/>
  </ui:with>
  <!-- VerticalLayoutData declarations ========================================================= -->
  <ui:with
    type='com.sencha.gxt.widget.core.client.container.VerticalLayoutContainer.VerticalLayoutData'
    field='buttonVLD'
  >
  </ui:with>
  <ui:with
    type='com.sencha.gxt.widget.core.client.container.VerticalLayoutContainer.VerticalLayoutData'
    field='fillRemainingVLD'
  >
    <ui:attributes
      height='1'
      width='1'/>
  </ui:with>
  <ui:with
    type='com.sencha.gxt.widget.core.client.container.VerticalLayoutContainer.VerticalLayoutData'
    field='sizeToContentsVLD'
  >
    <ui:attributes
      height='-1'
      width='1'/>
  </ui:with>
  <!-- ========================================================================================= -->
  <gxtcontainer:VerticalLayoutContainer ui:field='mainContainer'>
    <gxtcontainer:child layoutData='{sizeToContentsVLD}'>
      <gxtcontainer:HorizontalLayoutContainer ui:field='personContainer'>
        <gxtcontainer:VerticalLayoutContainer ui:field='personVLC'>
          <gxtform:FieldLabel itemId='Person.FirstName'/>
          <gxtform:FieldLabel itemId='Person.LastName'/>
          <gxtform:FieldLabel itemId='Person.DisplayName'/>
          <gxtform:FieldLabel itemId='Person.LoginName'/>
          <gxtform:FieldLabel itemId='Person.DateOfBirth'/>
        </gxtcontainer:VerticalLayoutContainer>
        <gxtcontainer:VerticalLayoutContainer>
          <gxtform:FieldLabel itemId='Person.EmailAddress'/>
          <gxtform:FieldLabel itemId='Person.PhoneNumberHome'/>
          <gxtform:FieldLabel itemId='Person.PhoneNumberMobile'/>
          <gxtform:FieldLabel itemId='Person.PhoneNumberOffice'/>
        </gxtcontainer:VerticalLayoutContainer>
      </gxtcontainer:HorizontalLayoutContainer>
    </gxtcontainer:child>
    <gxtcontainer:child layoutData='{sizeToContentsVLD}'>
      <gxtcontainer:HorizontalLayoutContainer ui:field="buttonContainer">
        <gxtcontainer:child layoutData="{buttonHLD}">
          <gxtbutton:TextButton
            ui:field='saveButton'
            text='Save Changes'/>
        </gxtcontainer:child>
        <gxtcontainer:child layoutData="{buttonHLD}">
          <gxtbutton:TextButton
            ui:field='undoButton'
            text='Undo Changes'/>
        </gxtcontainer:child>
      </gxtcontainer:HorizontalLayoutContainer>
    </gxtcontainer:child>
    <gxtcontainer:child layoutData='{fillRemainingVLD}'>
      <gxtcontainer:HorizontalLayoutContainer
        height='200'
        width='1000'
      >
        <gxtcontainer:child layoutData='{sizeToContentsHLD}'>
          <gxtcontainer:VerticalLayoutContainer>
            <gxtcontainer:child layoutData='{buttonVLD}'>
              <gxtbutton:TextButton
                ui:field='orgButton'
                text='Organizations'/>
            </gxtcontainer:child>
            <gxtcontainer:child layoutData='{buttonVLD}'>
              <gxtbutton:TextButton
                ui:field='personRelationshipButton'
                text='Relationships'/>
            </gxtcontainer:child>
            <gxtcontainer:child layoutData='{buttonVLD}'>
              <gxtbutton:TextButton
                ui:field='fosterHistoryButton'
                text='Foster History'/>
            </gxtcontainer:child>
          </gxtcontainer:VerticalLayoutContainer>
        </gxtcontainer:child>
        <gxtcontainer:child layoutData='{fillRemainingHLD}'>
          <gxt:ContentPanel
            height='200'
            width='800'
          >
            <gxtcontainer:CardLayoutContainer ui:field='cardLayoutContainer'>
              <gxt:ContentPanel
                ui:field='orgPanel'
                headerVisible='false'
                height='200'
                width='800'/>
              <gxt:ContentPanel
                ui:field='personRelationshipPanel'
                headerVisible='false'/>
              <gxt:ContentPanel
                ui:field='fosterHistoryPanel'
                headerVisible='false'/>
            </gxtcontainer:CardLayoutContainer>
          </gxt:ContentPanel>
        </gxtcontainer:child>
      </gxtcontainer:HorizontalLayoutContainer>
    </gxtcontainer:child>
  </gxtcontainer:VerticalLayoutContainer>
</ui:UiBinder>